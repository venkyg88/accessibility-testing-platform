{
  "name": "@a11y-scanner/backend",
  "version": "1.0.0",
  "description": "Backend API for A11y Scanner",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
