    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@a11y-scanner/config": "workspace:*",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.5.0",
    "prisma": "^5.5.0",
    "tsx": "^3.12.0",
    "typescript": "^5.2.0"
